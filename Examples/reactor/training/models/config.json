{
    "model": {
        "name": "TimeSeriesTransformer",
        "hyperparameters": {
            "encoder_layers": 2,
            "decoder_layers": 2,
            "train_split": 0.8,
            "learning_rate": 5e-4,
            "embedding_dim":60
        },
        "context_length": 3,
        "with_scaling": false,
        "final_regression_layers": 2
    },
    "data": {
        "num_data_points": 8
    },
    "training": {
        "train_size": 0.8,
        "random_seed": 42,
        "epochs": 100,
        "batch_size": 30,
        "loss": "mse",
        "continue_training" : false,
        "decay_epoch": 20,
        "decay_size": 0.9
    }
}
